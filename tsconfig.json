{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es2019", "esnext"],
    "outDir": "./dist/",
    "removeComments": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "strict": true
  },
  "include": ["./src", "./testing", "./typechain-types"],
  "files": ["./hardhat.config.ts"],
  "exclude": ["node_modules"]
}
